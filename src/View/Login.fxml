<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="384.0" prefWidth="524.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginController">
   <children>
      <Label fx:id="usernameLbl" layoutX="71.0" layoutY="123.0" prefHeight="33.0" prefWidth="119.0" text="Username:">
         <font>
            <Font name="Arial" size="24.0" />
         </font>
      </Label>
      <Label fx:id="passwordLbl" layoutX="71.0" layoutY="192.0" prefHeight="33.0" prefWidth="119.0" text="Password:">
         <font>
            <Font name="Arial" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTxtF" layoutX="260.0" layoutY="127.0" prefHeight="25.0" prefWidth="193.0">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <PasswordField fx:id="passwordTxtF" layoutX="260.0" layoutY="196.0" prefHeight="25.0" prefWidth="193.0">
         <font>
            <Font size="15.0" />
         </font>
      </PasswordField>
      <Button fx:id="loginBtn" layoutX="200.0" layoutY="286.0" mnemonicParsing="false" onAction="#loginBtnPressed" prefHeight="50.0" prefWidth="130.0" text="Log in">
         <font>
            <Font name="Arial Rounded MT Bold" size="14.0" />
         </font></Button>
      <Label fx:id="userLocLbl" contentDisplay="CENTER" layoutX="265.0" layoutY="253.0" prefHeight="33.0" prefWidth="171.0" text="Label">
         <font>
            <Font size="10.0" />
         </font></Label>
      <Label fx:id="invalidLoginLbl" alignment="CENTER" layoutX="133.0" layoutY="336.0" prefHeight="33.0" prefWidth="258.0" textAlignment="CENTER" textFill="#f50303">
         <font>
            <Font name="Corbel Light" size="15.0" />
         </font>
      </Label>
      <Label fx:id="loginTitleLbl" layoutX="49.0" layoutY="48.0" text="Appointment Scheduling">
         <font>
            <Font name="Arial Bold" size="36.0" />
         </font>
      </Label>
      <Label fx:id="locationLbl" layoutX="212.0" layoutY="261.0" text="Location:" />
   </children>
   <effect>
      <DropShadow />
   </effect>
</AnchorPane>
